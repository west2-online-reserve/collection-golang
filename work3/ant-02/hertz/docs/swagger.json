{
    "swagger": "2.0",
    "info": {
        "description": "A simple Todo app with Hertz + JWT + GORM",
        "title": "Hertz Todo API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/auth/finished/todo": {
            "put": {
                "description": "用户设置一个代办事件为完成状态",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "完成一个代办",
                "parameters": [
                    {
                        "type": "integer",
                        "format": "int64",
                        "description": "代办id",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "201": {
                        "description": "设置成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "设置失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "delete": {
                "description": "用户删除他的全部完成代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "删除全部完成代办",
                "responses": {
                    "204": {
                        "description": "删除成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "删除失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/finished/todos": {
            "get": {
                "description": "用户获取他的全部完成代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "获取全部完成代办",
                "responses": {
                    "200": {
                        "description": "返回代办信息",
                        "schema": {
                            "type": "array",
                            "items": {}
                        }
                    },
                    "500": {
                        "description": "获取失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "put": {
                "description": "用户设置全部代办事件为完成状态",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "完成全部代办",
                "responses": {
                    "201": {
                        "description": "设置成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "设置失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/key/todos": {
            "get": {
                "description": "用户根据关键字获取他的代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "模糊查询代办",
                "responses": {
                    "200": {
                        "description": "返回代办信息",
                        "schema": {
                            "type": "array",
                            "items": {}
                        }
                    },
                    "500": {
                        "description": "获取失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/notfinished/todo": {
            "put": {
                "description": "用户设置一个代办事件为未完成状态",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "取消完成一个代办",
                "parameters": [
                    {
                        "type": "integer",
                        "format": "int64",
                        "description": "代办id",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "201": {
                        "description": "设置成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "设置失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "delete": {
                "description": "用户删除他的全部未完成代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "删除未完成代办",
                "responses": {
                    "204": {
                        "description": "删除成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "删除失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/notfinished/todos": {
            "get": {
                "description": "用户获取他的全部未完成代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "获取全部未完成代办",
                "responses": {
                    "200": {
                        "description": "返回代办信息",
                        "schema": {
                            "type": "array",
                            "items": {}
                        }
                    },
                    "500": {
                        "description": "获取失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "put": {
                "description": "用户设置全部代办事件为未完成状态",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "未完成全部代办",
                "responses": {
                    "201": {
                        "description": "设置成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "设置失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/todo": {
            "delete": {
                "description": "用户删除他的一个代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "删除代办",
                "responses": {
                    "204": {
                        "description": "删除成功",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "删除失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/auth/todos": {
            "get": {
                "description": "用户获取他的全部代办事件",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Todo"
                ],
                "summary": "获取全部代办",
                "responses": {
                    "200": {
                        "description": "返回代办信息",
                        "schema": {
                            "type": "array",
                            "items": {}
                        }
                    },
                    "500": {
                        "description": "获取失败",
                        "schema": {
                            "type": "string"
                        }
                    }
